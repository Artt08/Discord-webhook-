{
    "manifest_version": 3,
    "name": "Discord Webhook Messenger",
    "version": "1.0",
    "description": "Send messages to Discord webhook from Chrome extension",
    "icons": {
        "128": "icons/icon.png"  // Path to your icon
    },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "storage"
    ]
}
